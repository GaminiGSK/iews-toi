{
    "hosting": {
        "site": "gksmart-ai-app",
        "public": "public",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "**",
                "run": {
                    "serviceId": "iews-toi",
                    "region": "asia-southeast1"
                }
            }
        ]
    }
}